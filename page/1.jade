doctype
html
	head
		meta(charset="utf-8")
		title
		style.
			ol li{float:left;margin:30px;list-style:none;}
			.active {color:red}
	body
		ul
			-for(var i = 0;i<listArr.length;i++)
				li #{listArr[i]}
		ol
			-for(var i =0;i < maxl;i++)
				li 
					if(i == index-1)
						a.active(href="http://localhost:8020/list/index?page=i+1") #{i+1}
					else
						a(href="http://localhost:8020/list/index?page="+(i+1)) #{i+1}
